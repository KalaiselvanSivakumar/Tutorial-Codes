{
    "manifest_version": 2,
    "name": "MovieFreak",
    "description": "Get movie details in your default tab",
    "version": "1",
    "author": "Kalai selvan Sivakumar",

    "browser_action": {
        "default_icon": "tab-icon.png",
        "default_title": "Good to see you",
        "default_popup": "popup.html"
    },

    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "permissions": ["activeTab"]
}